<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Ticket.
    -->
    <changeSet id="20201214152715-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="affects_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket_affects_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product_version"/>

        <addForeignKeyConstraint baseColumnNames="fixed_in_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket_fixed_in_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product_version"/>

    </changeSet>
</databaseChangeLog>
